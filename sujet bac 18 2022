t_moy = [14.9, 13.3, 13.1, 12.5, 13.0, 13.6, 13.7]
annees = [2013, 2014, 2015, 2016, 2017, 2018, 2019]

def mini(releve,date) :
    t_min = releve[0]
    ind_min = 0
    for i in range(len(releve)) :
        if t_min > releve[i] :
            t_min = releve[i]
            ind_min = i
    return t_min,date[ind_min]

print(mini(t_moy,annees))
